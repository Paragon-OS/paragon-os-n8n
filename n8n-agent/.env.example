# n8n Agent Configuration
# Copy this file to .env and fill in your values
#
# NOTE: n8n connection is now handled via pods.
# Use: npm run n8n:pod:start to start n8n in a podman pod.
# CLI commands automatically detect and connect to running pods.

# ============================================
# External Services
# ============================================

GOOGLE_GEMINI_API_KEY=your-key-here
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
QDRANT_URL=https://your-instance.cloud.qdrant.io:6333
QDRANT_API_KEY=your-qdrant-api-key

# ============================================
# MCP Server Configuration (for pods)
# ============================================

# Discord MCP (optional)
DISCORD_TOKEN=your-discord-token

# Telegram MCP (optional)
TELEGRAM_API_ID=your-api-id
TELEGRAM_API_HASH=your-api-hash
TELEGRAM_SESSION_STRING=your-session-string

# ============================================
# Test Configuration
# ============================================

# Log level (debug, info, warn, error)
LOG_LEVEL=info

# Test timeout (milliseconds)
TEST_TIMEOUT=300000