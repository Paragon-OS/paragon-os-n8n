    {
      "parameters": {
        "jsCode": "// Extract entity data from cache handler output and merge with entityConfig\nconst cacheHandlerOutput = $input.first().json;\nconst entityConfig = $json.entityConfig;\nconst query = $json.query || '';\n\n// Handle error cases from cache handler\nif (cacheHandlerOutput.error) {\n  throw new Error(`Failed to fetch entity data: ${cacheHandlerOutput.error}`);\n}\n\nif (!entityConfig) {\n  throw new Error(`entityConfig is missing. Available keys: ${JSON.stringify(Object.keys($json || {}))}`);\n}\n\nconst dataKey = entityConfig.dataKey;\n\n// Cache handler returns the data directly (from cache or fetch)\n// The data might be the array itself, or nested under dataKey\nlet entityData = cacheHandlerOutput;\n\n// If cacheHandlerOutput is an array, use it directly\n// Otherwise, check if it's nested under dataKey\nif (!Array.isArray(entityData) && entityData && entityData[dataKey] !== undefined) {\n  entityData = entityData[dataKey];\n}\n\n// Ensure entityData is an array\nif (!Array.isArray(entityData)) {\n  entityData = entityData ? [entityData] : [];\n}\n\n// Return data in format expected by search nodes\nreturn {\n  [dataKey]: entityData,\n  entityConfig: entityConfig,\n  query: query\n};"
      },
