    {
      "parameters": {
        "jsCode": "// Format final output based on search type\nconst input = $input.first().json;\n\n// Handle error cases\nif (input.error) {\n  throw new Error(input.error);\n}\n\nconst entityConfig = $('Prepare Entity Config').first().json.entityConfig;\nconst searchType = entityConfig.searchType;\n\n// Handle different output formats\nif (searchType === 'none' || entityConfig.skipQuery) {\n  // For skipQuery entities (like 'self'), return the data directly\n  const dataKey = entityConfig.dataKey;\n  return input[dataKey] || input;\n} else if (searchType === 'fuzzy') {\n  // Fuzzy search returns results with scores\n  return input;\n} else if (searchType === 'rag') {\n  // RAG search returns { results, count, answer }\n  // Extract results array\n  return input.results || input;\n}\n\n// Fallback: return as-is\nreturn input;"
      },
