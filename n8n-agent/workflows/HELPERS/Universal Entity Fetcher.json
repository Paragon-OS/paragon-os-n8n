    {
      "parameters": {
        "jsCode": "const mcpResponse = $input.first().json;\nconst config = $('Build Config').first().json.config;\n\n// Handle profile normalization (single object)\nif (config.normalizeType === 'profile') {\n  let profile = null;\n  try {\n    if (mcpResponse.result?.content?.[0]?.text) {\n      profile = JSON.parse(mcpResponse.result.content[0].text);\n    } else if (mcpResponse.myProfile) {\n      profile = mcpResponse.myProfile;\n    } else {\n      throw new Error('No profile found');\n    }\n    return { myProfile: profile };\n  } catch (error) {\n    return { myProfile: null, error: error.message };\n  }\n}\n\n// Handle tools normalization (object or array)\nif (config.normalizeType === 'tools') {\n  let tools = [];\n  try {\n    if (mcpResponse.result?.content?.[0]?.text) {\n      const parsed = JSON.parse(mcpResponse.result.content[0].text);\n      tools = parsed.tools || parsed;\n    } else if (mcpResponse.tools) {\n      tools = mcpResponse.tools;\n    }\n    \n    // Ensure tools is an array\n    if (!Array.isArray(tools)) {\n      tools = Object.values(tools);\n    }\n    \n    return {\n      output: { tools }\n    };\n  } catch (error) {\n    return {\n      error: error.message,\n      output: { tools: [] }\n    };\n  }\n}\n\n// Standard MCP Data Normalizer logic (contacts, guilds, chats, messages)\nconst { dataKey, mcpDataKey, mapper, errorMsg } = config;\nlet extractedData = [];\n\ntry {\n  // Extract data from MCP response\n  if (mcpResponse.result?.content?.[0]?.text) {\n    const parsed = JSON.parse(mcpResponse.result.content[0].text);\n    extractedData = mcpDataKey ? (parsed[mcpDataKey] || []) : (Array.isArray(parsed) ? parsed : []);\n  } else if (mcpResponse[dataKey]) {\n    extractedData = mcpResponse[dataKey];\n  }\n  \n  if (!Array.isArray(extractedData) || extractedData.length === 0) {\n    return { [dataKey]: [], error: errorMsg };\n  }\n  \n  // Apply mapper function\n  const mapperFunc = new Function('item', `return (${mapper})(item)`);\n  return { [dataKey]: extractedData.map(mapperFunc) };\n} catch (error) {\n  return { [dataKey]: [], error: error.message };\n}"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1544,
        400
      ],
