{
  "name": "paragonos-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "db:migrate": "bash scripts/supabase-wrapper.sh db reset || echo 'Supabase CLI not found. Run: npm run db:setup'",
    "db:migrate:status": "tsx scripts/migrate-supabase.ts",
    "db:status": "bash scripts/supabase-wrapper.sh migration list || echo 'Supabase CLI not found. Run: npm run db:setup'",
    "db:setup": "bash scripts/setup-supabase.sh",
    "db:start": "bash scripts/supabase-wrapper.sh start || echo 'Supabase CLI not found. Run: npm run db:setup'",
    "db:stop": "bash scripts/supabase-wrapper.sh stop || echo 'Supabase CLI not found'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "migrate-chat-data": "tsx scripts/migrate-chat-data.ts"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "tailwindStylesheet": "app/globals.css"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.31",
    "@ai-sdk/openai": "^2.0.46",
    "@assistant-ui/react": "^0.11.28",
    "@assistant-ui/react-ai-sdk": "^1.1.5",
    "@assistant-ui/react-markdown": "^0.11.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.49.2",
    "@types/lodash": "^4.17.21",
    "ai": "^5.0.65",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.22",
    "lodash": "^4.17.21",
    "lucide-react": "^0.545.0",
    "motion": "^12.23.22",
    "next": "15.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-shiki": "^0.9.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^4.0.13"
  }
}
