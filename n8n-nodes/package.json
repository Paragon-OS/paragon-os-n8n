{
  "name": "n8n-nodes-paragon-os",
  "version": "1.4.2",
  "description": "Custom n8n nodes for ParagonOS.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint nodes package.json",
    "lint:fix": "eslint nodes package.json --fix",
    "build": "tsc && gulp build:icons",
    "prepare": "npm run build",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/TextManipulation/TextManipulation.node.js",
      "dist/nodes/FuzzySearch/FuzzySearch.node.js",
      "dist/nodes/JsonDocumentLoader/JsonDocumentLoader.node.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ParagonTheDev/n8n-nodes-paragon-os.git"
  },
  "author": {
    "name": "ParagonTheDev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ParagonTheDev/n8n-nodes-paragon-os/issues"
  },
  "homepage": "https://github.com/ParagonTheDev/n8n-nodes-paragon-os#readme",
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.0",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.24.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "entities": "^6.0.0",
    "minisearch": "^7.2.0",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "string-strip-html": "^13.4.12"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
